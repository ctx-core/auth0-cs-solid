{
	"name": "@ctx-core/auth0-cs-solid",
	"version": "2.0.452",
	"description": "Auth0 solid-js components & ctx-core integration for client-server multipage apps & font-end single page web apps",
	"keywords": [
		"ctx-core",
		"object"
	],
	"homepage": "https://github.com/ctx-core/object#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/object/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/object.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.d.ts",
	"exports": {
		".": {
			"import": "./src/index.jsx",
			"types": "./src/index.d.ts"
		},
		"./spa": {
			"import": "./src/spa/index.jsx",
			"types": "./src/spa/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/auth0": "^39.0.39",
		"@ctx-core/dialog-ui-solid": "^2.2.176",
		"@ctx-core/dom": "^14.1.59",
		"@ctx-core/html": "^11.3.13",
		"@ctx-core/notyf": "^2.0.4",
		"@ctx-core/object": "^28.0.4",
		"@ctx-core/solid-js": "^1.4.5",
		"@ctx-core/solid-nanostores": "^17.0.4",
		"@ctx-core/ui-solid": "^15.0.7",
		"auth0-js": "^9.22.1",
		"solid-js": "^1.7.11"
	},
	"devDependencies": {
		"@types/auth0-js": "^9.21.0",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^3.12.7",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}